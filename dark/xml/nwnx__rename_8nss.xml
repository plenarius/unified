        <name>NWNX_Rename_SetPCNameOverride</name>
        <param>
          <type>object</type>
          <declname>oTarget</declname>
        </param>
        <param>
          <type>string</type>
          <declname>sNewName</declname>
        </param>
        <param>
          <type>string</type>
          <declname>sPrefix</declname>
          <defval>&quot;&quot;</defval>
        </param>
        <param>
          <type>string</type>
          <declname>sSuffix</declname>
          <defval>&quot;&quot;</defval>
        </param>
        <param>
          <type>int</type>
          <declname>iPlayerNameState</declname>
          <defval><ref refid="group__rename_1gafc3e340ecdad9db153e72054bbef9208" kindref="member">NWNX_RENAME_PLAYERNAME_DEFAULT</ref></defval>
        </param>
        <param>
          <type>object</type>
          <declname>oObserver</declname>
          <defval>OBJECT_INVALID</defval>
